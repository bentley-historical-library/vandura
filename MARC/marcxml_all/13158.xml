  <datafield tag="245" ind1="1" ind2="0">
    <subfield code="a">[Map of] Mackinac Island, Shewing [sic] the Surveys of Private Claims</subfield>
    <subfield code="h">[electronic resource],</subfield>
    <subfield code="f">1828.</subfield>
  </datafield>
